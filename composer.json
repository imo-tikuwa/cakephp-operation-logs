{
    "name": "imo-tikuwa/cakephp-operation-logs",
    "description": "OperationLogs plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "OperationLogs\\": "src/"
