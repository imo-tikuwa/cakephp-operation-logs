{
    "name": "imo-tikuwa/cakephp-operation-logs",
    "description": "OperationLogs plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": ">=4.3"
    },
    "autoload": {
        "psr-4": {
            "OperationLogs\\": "src/",
            "OperationLogs\\Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OperationLogs\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": "OperationLogs\\Console\\Installer::postInstall"
    }
}
